       "\n",
       "pop_lang_query = \"\"\"\n",
       "                 SELECT l.name as language_name, COUNT(*) as num_repos\n",
       "                 FROM `bigquery-public-data.github_repos.languages`,\n",
       "                     UNNEST(language) AS l\n",
       "                 GROUP BY language_name\n",
       "                 ORDER BY num_repos DESC\n",
       "                 \"\"\"\n",
       "\n",
       "```"
